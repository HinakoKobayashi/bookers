<h1>Books</h1>

<% @books.each do |book| %>

<span>
   <%= link_to book.title, book_path(book.id) %>  
</span>

<h2>New Books</h2>
<!-- form_with部分 -->
<%= form_with model: @book do |f| %>
  <h5>Title</h5>
  <%= f.text_field :title %>

  <h5>Body</h5>
  <%= f.text_area :body %>
  
  <h5></h5>
  <%= f.submit 'Create Books' %>
<% end %>

